{
  "manifest_version": 2,
  "name": "crawling test",
  "version": "0.1",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://hushit.live/*"
      ],
      "js": [
        "get_target_address.js"
      ]
    },
    {
      "matches": [
        "https://www.naver.com/"
      ],
      "js": [
        "crawling_target_content.js"
      ]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "set_auto_flag.js"
      ]
    }
  ]
}